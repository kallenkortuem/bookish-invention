{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.1.1",
    "generators": {
      "typescript-axios": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://www.mingweisamuel.com/riotapi-schema/openapi-3.0.0.yml",
        "output": "./generated",
        "skipValidateSpec": true,
        "templateDir": "templates/typescript-axios",
        "apiPackage": "api",
        "modelPackage": "model",
        "enablePostProcessFile": true,
        "additionalProperties": {
          "withInterfaces": true,
          "supportsES6": true,
          "withSeparateModelsAndApi": true,
          "npmName": "@kallenkortuem/bookish-invention",
          "npmRepository": "git@github.com:kallenkortuem/bookish-invention.git",
          "npmVersion": "0.0.1"
        }
      }
    }
  }
}
